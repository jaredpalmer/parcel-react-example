{
  "name": "parcel-react-example",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel ./src/index.html -p 8080 --open",
    "build": "yarn clear && cross-env NODE_ENV=production parcel build ./src/index.html --public-url /",
    "clear": "rm -rf dist .cache"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-preset-react-app": "^3.1.1",
    "cross-env": "^5.1.3",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  }
}
